   "source": [
    "class UTFCodeEncoder:\n",
    "\n",
    "    @staticmethod\n",
    "    def encode(b: bytes) -> int:\n",
    "        return int.from_bytes(b, byteorder='big')\n",
    "\n",
    "    @staticmethod\n",
    "    def decode(i: int) -> bytes:\n",
    "        return i.to_bytes(((i.bit_length() + 7) // 8), byteorder='big')"
   ]
  }
